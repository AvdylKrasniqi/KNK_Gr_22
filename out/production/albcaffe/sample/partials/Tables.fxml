<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="tablePane" onMouseClicked="#addTable" prefHeight="544.0" prefWidth="599.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.kamarieri.Controller">
   <children>
      <GridPane fx:id="tablesGrid" onContextMenuRequested="#openContextMenu" onMouseClicked="#updateValues" prefHeight="499.0" prefWidth="602.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
      <AnchorPane layoutY="510.0" prefHeight="40.0" prefWidth="602.0" style="-fx-background-color: rgba(0, 31, 63, 1);">
         <children>
            <Label fx:id="numberOfTable" layoutX="24.0" layoutY="12.0" text="%TableNumber" textFill="WHITE">
               <font>
                  <Font name="Tibetan_Machine_Uni" size="13.0" />
               </font>
            </Label>
            <Label fx:id="numberOfActiveTables" layoutX="210.0" layoutY="12.0" prefHeight="16.0" prefWidth="193.0" text="%OccupiedTables" textFill="WHITE">
               <font>
                  <Font name="Tibetan_Machine_Uni" size="13.0" />
               </font>
            </Label>
            <Label fx:id="brutoEarnings" layoutX="420.0" layoutY="12.0" prefHeight="16.0" prefWidth="193.0" text="%Earnings" textFill="WHITE">
               <font>
                  <Font name="Tibetan_Machine_Uni" size="13.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
