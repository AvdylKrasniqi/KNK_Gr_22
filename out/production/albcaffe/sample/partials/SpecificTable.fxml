<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PartialControllers.TableScreenController">
   <children>
      <VBox prefHeight="551.0" prefWidth="613.0">
         <children>
            <Label fx:id="tableTitle" text="Specific Table">
               <font>
                  <Font name="Ubuntu Bold" size="30.0" />
               </font>
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </padding>
               <VBox.margin>
                  <Insets left="175.0" />
               </VBox.margin>
            </Label>
            <Label text="Is it occupied?">
               <font>
                  <Font name="Samanata" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="15.0" left="215.0" top="30.0" />
               </VBox.margin>
            </Label>
            <CheckBox fx:id="yesCheck" disable="true" mnemonicParsing="false" text="Yes">
               <VBox.margin>
                  <Insets left="220.0" />
               </VBox.margin>
            </CheckBox>
            <CheckBox fx:id="noCheck" disable="true" mnemonicParsing="false" text="No">
               <VBox.margin>
                  <Insets left="300.0" top="-17.0" />
               </VBox.margin>
            </CheckBox>
            <ComboBox fx:id="menuCombo" prefWidth="150.0">
               <VBox.margin>
                  <Insets left="215.0" top="30.0" />
               </VBox.margin>
            </ComboBox>
            <Label text="Quantity">
               <VBox.margin>
                  <Insets left="210.0" top="25.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="quantityField" maxHeight="100.0" maxWidth="100.0">
               <VBox.margin>
                  <Insets left="280.0" top="-20.0" />
               </VBox.margin>
            </TextField>
            <Button mnemonicParsing="false" onAction="#addItem" text="Add">
               <VBox.margin>
                  <Insets left="260.0" top="15.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#goToDetails" text="Details">
               <VBox.margin>
                  <Insets bottom="-25.0" left="180.0" top="30.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#clearTable" text="Clear Table">
               <VBox.margin>
                  <Insets left="300.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#saveTable" text="Save">
               <VBox.margin>
                  <Insets left="250.0" top="30.0" />
               </VBox.margin>
            </Button>
            <Label fx:id="tableCapacity" text="Kjo tavoline ka 4 karrika">
               <VBox.margin>
                  <Insets left="150.0" top="50.0" />
               </VBox.margin>
               <font>
                  <Font name="Garuda" size="26.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
